{
  "name": "vankosoft/vs_django",
  "version": "0.1.0",
  "author": "Ivan I. Atanasov",
  "summary": "Installs, configures, and manages django environements.",
  "license": "Apache-2.0",
  "source": "git://github.com/iatanasov77/puppet-django.git",
  "project_page": "http://github.com/iatanasov77/puppet-django",
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 4.13.1 < 7.0.0"
    },
    {
      "name": "vankosoft/vs_devenv",
      "version_requirement": ">= 0.1.0"
    },
    {
      "name": "vankosoft/vs_lamp",
      "version_requirement": ">= 0.1.0"
    }
  ],
  "operatingsystem_support": [
    {
